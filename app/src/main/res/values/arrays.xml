    </string-array>

    <array name="ld_activityScreenIcons">
        <item>@drawable/ic_home_outline_grey600_24dp</item>
        <item>@drawable/ic_account_outline_grey600_24dp</item>
        <item>@drawable/ic_email_outline_grey600_24dp</item>
        <item>@drawable/ic_cart_outline_grey600_24dp</item>
        <item>@drawable/ic_cart_outline_grey600_24dp</item>
